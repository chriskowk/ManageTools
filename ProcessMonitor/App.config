<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <!--adplus的路径-->
    <add key="adplusPath" value="C:\Program Files (x86)\Windows Kits\10\Debuggers\x64"/>
    <!--dmp文件保存路径-->
    <add key="outputPath" value="C:\Dumps"/>
    <!--要抓dump的进程的名字，可用部分名字，不用完整的-->
    <add key="processName" value="JobController"/>
  </appSettings>
</configuration>